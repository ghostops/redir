{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-typescript"
  ],
  "plugins": [
    "@babel/transform-runtime",
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread",
    ["module-resolver", {
      "alias": {
        "functions": "./build/functions",
        "helpers": "./build/helpers.js"
      }
    }]
  ]
}
