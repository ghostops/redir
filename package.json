{
    "name": "serverless-starter",
    "version": "1.0.0",
    "description": "JF Serverless Starter",
    "main": "handler.js",
    "author": "Ludvig Larsendahl",
    "license": "MIT",
    "private": true,
    "scripts": {
        "start": "yarn build --watch & serverless offline",
        "deploy": "yarn build && yarn deploy-dev",
        "deploy-dev": "serverless deploy --aws-profile jf_dev --stage dev",
        "deploy-prod": "serverless deploy --aws-profile jf_live --stage live",
        "build": "babel ./src --out-dir build --extensions '.ts,.tsx'",
        "tsfix": "yarn tslint --fix --project ./"
    },
    "devDependencies": {
        "@babel/cli": "^7.0.0",
        "@babel/core": "^7.0.0",
        "@babel/plugin-proposal-class-properties": "^7.4.4",
        "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
        "@babel/plugin-transform-runtime": "^7.4.4",
        "@babel/preset-env": "^7.4.5",
        "@babel/preset-es2015": "^7.0.0-beta.53",
        "@babel/preset-typescript": "^7.3.3",
        "@types/aws-lambda": "^8.10.17",
        "@types/aws-sdk": "^2.7.0",
        "@types/lodash": "^4.14.119",
        "@types/node": "^10.12.18",
        "babel-plugin-module-resolver": "^3.1.1",
        "serverless-offline": "^5.0.1",
        "tslint": "^5.12.0",
        "tslint-config-airbnb": "^5.11.1",
        "typescript": "^3.2.2"
    },
    "dependencies": {
        "@babel/runtime": "^7.4.5",
        "aws-sdk": "^2.382.0",
        "axios": "^0.18.0",
        "lodash": "^4.17.11"
    }
}
